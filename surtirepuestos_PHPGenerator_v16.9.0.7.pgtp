            <DataColumns>
              <Column name="utilidad" label="Utilidad" type="float" format="$ ##,###,###"/>
              <Column name="totalventas" label="Total Venta" type="float" format="$ ##,###,###"/>
            </DataColumns>
            <SelectStatement>
SELECT 
  to_char(facturar_ventas.fecha_hora,'YYYY - Mon') as mes, 
  sum(facturar_ventas.utilidad_en_venta) as utilidad,
  sum(facturar_ventas.valor_total_pagar) as totalventas
FROM   public.facturar_ventas
  GROUP BY    to_char(facturar_ventas.fecha_hora,'YYYY - Mon')
  ORDER BY   to_char(facturar_ventas.fecha_hora,'YYYY - Mon') asc
  limit 12            </SelectStatement>
          </Chart>
          <Chart name="utilidad_anual" title="Utilidad Anual" type="0" width="10" domainColumnName="anno" domainColumnLabel="anno" domainColumnType="string">
            <DataColumns>
